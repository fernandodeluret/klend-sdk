{
  "name": "@kamino-finance/klend-sdk",
  "version": "4.0.1",
  "description": "Typescript SDK for interacting with the Kamino Lending (klend) protocol",
  "repository": {
    "type": "git",
    "url": "git+github.com:Kamino-Finance/klend-sdk.git"
  },
  "author": "Kamino Finance",
  "homepage": "https://kamino.finance",
  "bugs": {
    "url": "https://github.com/Kamino-Finance/klend-sdk/issues"
  },
  "keywords": [
    "solana",
    "anchor",
    "klend",
    "kamino"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/**/*.ts"
  ],
  "license": "MIT",
  "scripts": {
    "docs": "typedoc",
    "prepublish": "yarn build",
    "build": "rm -rf dist/; tsc",
    "build:test": "yarn tsc --project ./tests/tsconfig.json",
    "test": "npx ts-mocha tests/**/*.test.ts --exit",
    "setup": "npx ts-mocha tests/setup2.test.ts --exit",
    "watch": "tsc --watch",
    "coverage": "jest --coverage",
    "clean": "rm -rf dist/",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "yarn clean && yarn clean:deps",
    "lint": "yarn prettier --check .; yarn eslint .",
    "lint:fix": "yarn prettier --write .; yarn eslint . --fix",
    "codegen": "yarn anchor-client-gen src/idl.json ./src/idl_codegen/ --program-id KLend2g3cP87fffoy8q1mQqGKjrxjC8boSyAYavgmjD",
    "codegen-kvault": "yarn anchor-client-gen src/idl_kamino_vault.json ./src/idl_codegen_kamino_vault/ --program-id kvauTFR8qm1dhniz6pYuBZkuene3Hfrs1VQhVRgCNrr",
    "codegen-jupiter-perps": "yarn anchor-client-gen src/jupiter_perps_idl.json ./src/idl_codegen_jupiter_perps/ --program-id PERPHjGBqRHArX4DySjwM6UJHiR3sWAatqfdBS2qQJu",
    "start-validator": "solana-test-validator $(./deps/test-validator-params.sh)",
    "start-validator-and-test": "yarn start-server-and-test 'yarn start-validator' http://127.0.0.1:8899/health test",
    "dump-programs": "./deps/dump-from-mainnet.sh",
    "kamino-manager": "ts-node src/client_kamino_manager.ts"
  },
  "dependencies": {
    "@bokuweb/zstd-wasm": "^0.0.20",
    "@coral-xyz/anchor": "^0.28.0",
    "@coral-xyz/borsh": "^0.28.0",
    "@kamino-finance/farms-sdk": "^2.0.3",
    "@kamino-finance/kliquidity-sdk": "^6.2.1",
    "@kamino-finance/scope-sdk": "^8.0.2",
    "@pythnetwork/client": "^2.5.1",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.87.6",
    "@switchboard-xyz/sbv2-lite": "^0.1.0",
    "axios": "^1.6.8",
    "base58-js": "^2.0.0",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "buffer-layout": "^1.2.2",
    "commander": "^9.3.0",
    "decimal.js": "^10.4.3",
    "exponential-backoff": "^3.1.1"
  },
  "devDependencies": {
    "@orca-so/sdk": "^1.2.25",
    "@orca-so/whirlpool-sdk": "^0.4.2",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.54",
    "@types/bn.js": "^5.1.1",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.5",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^10.0.6",
    "@types/node-fetch": "^2.6.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "anchor-client-gen": "^0.28.1",
    "chai": "^4.3.7",
    "chai-bignumber": "^3.1.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "jest": "^27.4.3",
    "jest-fetch-mock": "^3.0.3",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^28.0.7",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.22.10",
    "typescript": "^5.4.5",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "resolutions": {
    "@kamino-finance/farms-sdk": "^2.0.3",
    "@kamino-finance/kliquidity-sdk": "^6.2.1"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
